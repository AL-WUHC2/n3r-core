[updatePrizeItemMd5]
UPDATE PRIZE_ITEM
SET    ITEM_MD5 = #?#
WHERE  ACTIVITY_ID = #?#
AND    ITEM_ID = #?#
AND    (ITEM_MD5 IS NULL OR ITEM_MD5 <> #?#)

[updatePrizeItemAssign]
DELETE PRIZE_ITEM_ASSIGN
WHERE  ACTIVITY_ID = #?#
AND    ITEM_ID = #?#
;

INSERT INTO PRIZE_ITEM_ASSIGN(ACTIVITY_ID, ITEM_ID, ASSIGN_TIME_FROM,
    ASSIGN_TIME_TO, LUCK_TIME, ASSIGN_NUM, AVAILABLE_NUM)
VALUES(#?#)