[increaseDayMax]
MERGE INTO PRIZE_PROPS T USING DUAL
ON (T.ACTIVITY_ID = #1# AND T.PROPERTY = #2#)
WHEN NOT MATCHED THEN
INSERT (ACTIVITY_ID, PROPERTY, STR_VALUE, NUM_VALUE)
VALUES(#1#, #2#, NULL, 0);


UPDATE PRIZE_PROPS
SET NUM_VALUE = NUM_VALUE + 1
WHERE ACTIVITY_ID = ##
AND PROPERTY = ##
AND NUM_VALUE < ##