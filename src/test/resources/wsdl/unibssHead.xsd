          <xs:simpleType> 
            <xs:restriction base="xs:string"> 
              <xs:minLength value="0"/>  
              <xs:maxLength value="32"/> 
            </xs:restriction> 
          </xs:simpleType> 
        </xs:element>  
        <xs:element name="TRANS_IDH" minOccurs="0"> 
          <xs:annotation> 
            <xs:documentation>服务提供方流水号</xs:documentation> 
          </xs:annotation>  
          <xs:simpleType> 
            <xs:restriction base="xs:string"> 
              <xs:minLength value="0"/>  
              <xs:maxLength value="30"/> 
            </xs:restriction> 
          </xs:simpleType> 
        </xs:element>  
        <xs:element name="PROCESS_TIME"> 
          <xs:annotation> 
            <xs:documentation>服务处理时间</xs:documentation> 
          </xs:annotation>  
          <xs:simpleType> 
            <xs:restriction base="xs:string"> 
              <xs:length value="14"/> 
            </xs:restriction> 
          </xs:simpleType> 
        </xs:element>  
        <xs:element name="RESPONSE" minOccurs="0"> 
          <xs:complexType> 
            <xs:sequence> 
              <xs:element name="RSP_TYPE"> 
                <xs:annotation> 
                  <xs:documentation>应答/错误类型</xs:documentation> 
                </xs:annotation>  
                <xs:simpleType> 
                  <xs:restriction base="xs:string"> 
                    <xs:length value="1"/> 
                  </xs:restriction> 
                </xs:simpleType> 
              </xs:element>  
              <xs:element name="RSP_CODE"> 
                <xs:annotation> 
                  <xs:documentation>应答/错误代码</xs:documentation> 
                </xs:annotation>  
                <xs:simpleType> 
                  <xs:restriction base="xs:string"> 
                    <xs:length value="4"/> 
                  </xs:restriction> 
                </xs:simpleType> 
              </xs:element>  
              <xs:element name="RSP_DESC"> 
                <xs:annotation> 
                  <xs:documentation>应答/错误描述</xs:documentation> 
                </xs:annotation>  
                <xs:simpleType> 
                  <xs:restriction base="xs:string"> 
                    <xs:minLength value="0"/>  
                    <xs:maxLength value="512"/> 
                  </xs:restriction> 
                </xs:simpleType> 
              </xs:element> 
            </xs:sequence> 
          </xs:complexType> 
        </xs:element>  
        <xs:element name="COM_BUS_INFO"> 
          <xs:complexType> 
            <xs:sequence> 
              <xs:element name="OPER_ID"> 
