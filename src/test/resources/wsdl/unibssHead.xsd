                <xs:simpleType> 
                  <xs:restriction base="xs:string"> 
                    <xs:length value="2"/> 
                  </xs:restriction> 
                </xs:simpleType> 
              </xs:element> 
            </xs:sequence> 
          </xs:complexType> 
        </xs:element>  
