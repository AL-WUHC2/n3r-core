          <xs:annotation> 
            <xs:documentation>测试标记</xs:documentation> 
          </xs:annotation>  
          <xs:simpleType> 
            <xs:restriction base="xs:string"> 
              <xs:length value="1"/> 
            </xs:restriction> 
          </xs:simpleType> 
